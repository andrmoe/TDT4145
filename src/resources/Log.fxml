<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>

<VBox alignment="CENTER" maxHeight="600.0" maxWidth="335.0" minHeight="600.0" minWidth="335.0" prefHeight="600.0" prefWidth="335.0" spacing="50.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller = "controller.LogController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <DatePicker fx:id="workoutDatePicker" />
      <Line endX="100.0" startX="-100.0" />
      <ChoiceBox fx:id="groupChoiceBox" prefWidth="150.0" />
      <ChoiceBox fx:id="excerciseChoiceBox" prefWidth="150.0" />
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0">
         <children>
            <TextField fx:id="kiloText" prefWidth="100.0" promptText="Antall kilo" />
            <TextField fx:id="settText" layoutX="10.0" layoutY="10.0" prefWidth="100.0" promptText="Antall sett" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#handleAddExercise" text="Legg til øvelse" />
      <Line endX="100.0" layoutX="178.0" layoutY="146.0" startX="-100.0" />
      <Button mnemonicParsing="false" onAction="#handleCompleteWorkout" text="Fullfør treningsøkt" />
   </children>
</VBox>
